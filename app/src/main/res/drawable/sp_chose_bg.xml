<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/chage_nor" />
            <corners android:radius="20dp" />
            <stroke android:width="2dp" android:color="@color/chage_nor" />
            <padding android:bottom="2dp" android:left="20dp" android:right="20dp" android:top="2dp" />
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">
            <solid android:color="@android:color/white" />
            <corners android:radius="20dp" />
            <stroke android:width="1dp" android:color="@color/htranslucence" />
            <padding android:bottom="2dp" android:left="20dp" android:right="20dp" android:top="2dp" />

        </shape>
    </item>


</selector>